<!DOCTYPE html>
<html>
<head>
  <title>University Projects</title>
</head>
<body>
<table>
  <tr>
    <th>Title:</th>
    <th>Text:</th>
  </tr>

  <% @projects.each do |project| %>
    <tr>
      <td> <%= project.title%> </td>
      <td> <%= project.description%> </td>
      <td><%= link_to "Show", project_path(project) %></td>
      <td><%= link_to "New", new_project_path %></td>
      <td><%= link_to "Edit", edit_project_path(project) %> </td>
    </tr>
    <% end %>

</table>
</body>
</html>
